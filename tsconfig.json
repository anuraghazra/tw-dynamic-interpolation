{
  "compilerOptions": {
    "outDir": "dist",
    "jsx": "react",
    "downlevelIteration": true,
    "noImplicitAny": false,
    "target": "ES2015",
    "module": "commonjs",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true
  },
  "exclude": ["examples/**/*.tsx", "__tests__"]
}
